{
  "name": "fleetcore-website",
  "version": "1.0.0",
  "description": "FleetCore Enterprise Website - Agentic Maritime Intelligence Platform",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "react-snap",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "reactSnap": {
    "inlineCss": true,
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": false
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/",
      "/solutions",
      "/platform",
      "/about",
      "/contact",
      "/privacy-policy"
    ],
    "userAgent": "ReactSnap",
    "fixWebpackChunksIssue": false,
    "skipThirdPartyRequests": true
  },
  "keywords": [
    "maritime",
    "intelligence",
    "agentic",
    "maintenance",
    "enterprise",
    "fleetcore"
  ],
  "author": "FleetCore Team",
  "license": "MIT",
  "dependencies": {
    "@types/three": "^0.180.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.20.1",
    "recharts": "^3.2.1",
    "tailwind-merge": "^2.1.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
